</footer>

<script>
  // ── Page Routing ──
  function showPage(pageId) {
    document.querySelectorAll('.page').forEach(p => p.classList.remove('active'));
    document.getElementById('page-' + pageId).classList.add('active');
    window.scrollTo({ top: 0, behavior: 'smooth' });
  }

  document.querySelectorAll('.page-nav a, .nav-links a').forEach(link => {
    link.addEventListener('click', e => {
      e.preventDefault();
      showPage(link.dataset.page);
    });
  });

  // ── Logo click ──
  document.querySelector('.nav-logo').addEventListener('click', () => showPage('home'));

  // ── Back buttons on sub-pages ──
  document.querySelectorAll('.back-home').forEach(btn => {
    btn.addEventListener('click', e => { e.preventDefault(); showPage('home'); });
  });

  // ── Back to Brag Book ──
  document.querySelectorAll('.back-brag').forEach(btn => {
    btn.addEventListener('click', e => { e.preventDefault(); showPage('outbound'); });
  });
</script>
</body>
</html>
